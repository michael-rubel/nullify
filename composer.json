{
    "name": "michael-rubel/nullify",
    "description": "Convert empty data of any type to null.",
    "keywords": [
        "michael-rubel",
        "nullify"
    ],
    "homepage": "https://github.com/michael-rubel/nullify",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Rubel",
            "email": "michael@laravel.software",
            "homepage": "https://laravel.software",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpstan/phpstan": "^1.9.2",
        "illuminate/support": "^9.0",
        "symfony/var-dumper": "^5.4|^6.1",
        "roave/backward-compatibility-check": "^7.0|^8.0",
        "roave/infection-static-analysis-plugin": "^1.25"
    },
    "autoload": {
        "psr-4": {
            "MichaelRubel\\Nullify\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MichaelRubel\\Nullify\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "infection/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
